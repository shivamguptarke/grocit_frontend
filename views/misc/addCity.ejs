<%- include('header') -%> 
        <!-- Layout container -->
        <div class="layout-page">
          <!-- Content wrapper -->
          <div class="content-wrapper">
            <!-- Content -->

            <div class="container-xxl flex-grow-1 container-p-y">
              <h4 class="fw-bold py-3 mb-4"><span class="text-muted fw-light">City /</span> Add City</h4>

              <div class="card mb-4">
                <h5 class="card-header">City Details</h5>
                <!-- Account -->
                <hr class="my-0">
                <div class="card-body">
                  <form id="formAccountSettings" method="POST" onsubmit="addCity">
                    <div class="row">
                      <div class="mb-3 col-md-6">
                        <label for="name" class="form-label">City Name</label>
                        <input class="form-control" type="text" id="name" name="name" value="" autofocus="">
                      </div>
                      <div class="mb-3 col-md-6">
                        <label class="form-label" for="countryid">Country</label>
                        <select id="countryid" class="select2 form-select" name="countryid">
                          <option value="">Select Country</option>
                          <% itemsArray.forEach(i => { %>
                            <option value="<%- i._id %>"><%- i.name %></option>
                          <% }) %>
                        </select>
                      </div>
                      <div class="mb-3 col-md-6">
                        <label class="form-label" for="stateid">State</label>
                        <select id="stateid" class="select2 form-select" name="stateid">
                          <option value="">Select State</option>
                          <% stateArray.forEach(i => { %>
                            <option value="<%- i._id %>"><%- i.name %></option>
                          <% }) %>
                        </select>
                      </div>
                      <div class="mb-3 col-md-6">
                        <label for="pincode" class="form-label">Pincode</label>
                        <input class="form-control" type="text" id="pincode" name="pincode" value="" autofocus="">
                      </div>
                      <div class="form-check" style="margin: 2% 1%;">
                        <input class="form-check-input" type="checkbox" value='1' id="status" checked="" name="status">
                        <label class="form-check-label" for="status">Visible </label>
                      </div>
                    </div>
                    <div class="mt-2">
                      <button type="submit" class="btn btn-primary me-2">Add City</button>
                      <button type="reset" class="btn btn-outline-secondary">Cancel</button>
                    </div>
                  </form>
                </div>
                <!-- /Account -->
              </div>

            <!-- / Content -->
            
<%- include('footer') -%>
        